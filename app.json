{
  "expo": {
    "scheme": "SmartMed App",
    "name": "SmartMed App",
    "slug": "SmartMed App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_resize.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "buildNumber": "1.0.0",
      "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
